<query id="getModuleSrlByMid" action="select">
    <tables>
        <table name="modules" />
    </tables>
    <columns>
        <column name="module_srl" />
    </columns>
    <conditions>
        <condition operation="equal" column="site_srl" var="site_srl" />
        <condition operation="in" column="mid" var="mid" notnull="notnull" pipe="and" />
    </conditions>
</query>

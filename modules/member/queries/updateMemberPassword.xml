<query id="updateMemberPassword" action="update">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="password" var="password" />
        <column name="denied" var="denied" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" filter="number" />
    </conditions>
</query>

<query id="updateMemberDevice" action="update">
    <tables>
        <table name="member_devices" />
    </tables>
    <columns>
        <column name="device_token" var="new_token" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="device_token" var="old_token" notnull="notnull" />
    </conditions>
</query>

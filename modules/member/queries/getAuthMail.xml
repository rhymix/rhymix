<query id="getAuthMail" action="select">
    <tables>
        <table name="member_auth_mail" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" />
        <condition operation="equal" column="auth_key" var="auth_key" pipe="and" />
    </conditions>
</query>

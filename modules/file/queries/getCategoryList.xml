<query id="getCategoryList" action="select">
  <tables>
    <table name="categories" />
  </tables>
  <conditions>
    <condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
  </conditions>
  <navigation>
    <index var="sort_index" order="asc" />
  </navigation>
</query>

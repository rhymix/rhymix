<query id="getLayoutMenu" action="select">
  <tables>
    <table name="layout_menu" />
  </tables>
  <columns>
    <column name="*" />
  </columns>
  <conditions>
    <condition operation="equal" column="menu_srl" var="menu_srl" filter="number" notnull="notnull" />
  </conditions>
  <navigation>
    <index var="sort_index" default="listorder" order="asc" />
  </navigation>
</query>

<filter name="widget_login" module="member" act="procMemberLogin">
  <form>
    <node target="user_id" required="true" filter="user_id" />
    <node target="password" required="true" />
  </form>
  <parameter />
  <response callback_func="completeLogin">
    <tag name="error" />
    <tag name="message" />
  </response>
</filter>
